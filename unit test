-- Unit test for View_InstituteInfoWithStatus
-- Verify that the view returns expected columns
SELECT * FROM INFORMATION_SCHEMA.COLUMNS WHERE TABLE_NAME = 'View_InstituteInfoWithStatus';

-- Unit test for GetCoursesByInstituteID stored procedure
-- Verify that the stored procedure returns courses for a given institute ID
DECLARE @InstituteID INT = 1; -- Specify the institute ID to test
EXEC GetCoursesByInstituteID @InstituteID;

